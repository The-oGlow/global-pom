        <jacoco.itReportFile>${jacoco.destFolder}/jacoco-it.exec</jacoco.itReportFile>
        <jacoco.commonReportFile>${jacoco.destFolder}/jacoco.common</jacoco.commonReportFile>
        <!-- -->
        <jacoco.reportTitle>Unit-Test Coverage-Report for ${project.name}</jacoco.reportTitle>
        <jacoco.itReportTitle>Integration-Test Coverage-Report for ${project.name}</jacoco.itReportTitle>
