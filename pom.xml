        <sonar.verbose>false</sonar.verbose>
        <sonar.sources>src/main,pom.xml,*.adoc,README.*</sonar.sources>
        <sonar.tests>src/test</sonar.tests>
        <!-- -->
        <sonar.coverage.jacoco.xmlReportPaths>
            ${jacoco.reportOutputFolder}/jacoco.xml,${jacoco.itReportOutputFolder}/jacoco.xml
